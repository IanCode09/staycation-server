<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="nav-home-tab">
  <div class="row">
    <% for(i = 0; i < item.imageId.length; i++) { %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img src='http://localhost:3000/<%= item.imageId[i].imageUrl %>' alt="imageItem"
            style="width: 100%; display: block">
        </div>
      </div>
    </div>
    <% } %>
  </div>
</div>